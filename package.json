{
  "name": "keil-vscode",
  "displayName": "Keil V",
  "description": "An assistant for Keil uVision 5",
  "version": "0.0.3-beta.0",
  "publisher": "jiyun-tech",
  "author": {
    "name": "www.jiyun.tech",
    "email": "mboss0@163.com"
  },
  "keywords": [
    "keil",
    "mdk",
    "stm32"
  ],
  "homepage": "https://github.com/mbossX/keil-vscode",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "icon": "res/icon.png",
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mbossX/keil-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/mbossX/keil-vscode/issues",
    "email": "mboss0@163.com"
  },
  "contributes": {
    "icons": {
      "keil-v-build": {
        "description": "build icon",
        "default": {
          "fontPath": "./res/iconfont.woff",
          "fontCharacter": "\\e615"
        }
      },
      "keil-v-rebuild": {
        "description": "rebuild icon",
        "default": {
          "fontPath": "./res/iconfont.woff",
          "fontCharacter": "\\e616"
        }
      },
      "keil-v-download": {
        "description": "download icon",
        "default": {
          "fontPath": "./res/iconfont.woff",
          "fontCharacter": "\\e618"
        }
      },
      "keil-v-switch": {
        "description": "switch icon",
        "default": {
          "fontPath": "./res/iconfont.woff",
          "fontCharacter": "\\e617"
        }
      }
    },
    "configuration": [
      {
        "title": "%extension.name%",
        "properties": {
          "keil-vscode.MDK.Uv4Path": {
            "type": "string",
            "scope": "machine",
            "markdownDescription": "%extension.settings.mdk%",
            "default": "null"
          },
          "keil-vscode.C51.Uv4Path": {
            "type": "string",
            "scope": "machine",
            "markdownDescription": "%extension.settings.c51%",
            "default": "null"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "keil-vscode.project.build",
        "title": "%extension.commands.build.title%"
      },
      {
        "command": "keil-vscode.project.rebuild",
        "title": "%extension.commands.rebuild.title%"
      },
      {
        "command": "keil-vscode.project.download",
        "title": "%extension.commands.download.title%"
      },
      {
        "command": "keil-vscode.project.switch",
        "title": "%extension.commands.switch.title%"
      }
    ],
    "problemMatchers": [
      {
        "name": "armcc",
        "fileLocation": "autoDetect",
        "pattern": [
          {
            "regexp": "^([^\\(]+)\\(([\\d]+)\\):\\s+(error|warning):\\s+#([\\d\\w-]+):\\s+(.+)$",
            "file": 1,
            "location": 2,
            "severity": 3,
            "code": 4,
            "message": 5
          }
        ]
      },
      {
        "name": "gcc",
        "fileLocation": "autoDetect",
        "pattern": [
          {
            "regexp": "^(.+):(\\d+):(\\d+):\\s+(\\w+):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "login": "vsce login jiyun-tech i6b7aetlizyjsk37zhbt7y3cpuoqyjlrua7iqsvomppliqc7ukna",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^3.10.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^6.2.3",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "vsce": "2.9.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "xml2js": "^0.4.23"
  },
  "dependencies": {}
}
